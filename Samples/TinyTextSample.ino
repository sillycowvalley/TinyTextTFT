TinyTextTFT tft = TinyTextTFT(TFT_CS, TFT_DC, TFT_RST);

void setup() 
{
  Serial.begin(115200);
  delay(500);
  Serial.println();
  Serial.println("TinyText Test!"); 

  tft.Begin();
  
  Serial.println(F("Done!"));
}

unsigned long testFillScreen(uint16_t color)
{
  unsigned long start = micros();
  tft.FillScreen(color);
  return micros() - start;
}

